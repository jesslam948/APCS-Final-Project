
import java.util.*;
public class CatSelections {
	private Cat blackCat;
	private Cat mottledCat;
	private ArrayList<Cat> cats;
	private Cat tabbyCat;
	public CatSelections(){
		blackCat = new Cat ("black", 5,5,5,20);
		mottledCat = new Cat ("mottled", 5, 5, 5, 20);
		tabbyCat = new Cat ("tabby", 5, 5, 5, 20);
	}
	public ArrayList<Cat> getCats(){
		return cats;
	} 
	public void setCats(ArrayList<Cat> list){
		cats = list;
	}
}
